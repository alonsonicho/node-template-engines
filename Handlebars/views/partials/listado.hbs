<h2 style="color:crimson;">Historial</h2>


{{#if DB_PRODUCTOS}}
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Precio</th> <th>URL</th> </tr>   
            {{#each DB_PRODUCTOS}}
            <tr> 
                <td>{{this.nombre}}</td>
                <td>{{this.precio}}</td>
                <td>{{this.foto}}</td>
            </tr>
            {{/each}}
        </table>
    </div>
{{else}}
    <h3 class="alert alert-danger">No se encontraron dato</h3>
{{/if}}


